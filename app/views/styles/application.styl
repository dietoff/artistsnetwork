// nib is a powerful lib for stylus that provides cross-browser css3 mixins.
// No more vendor prefixes. http://visionmedia.github.com/nib/
@import 'nib'

*
	box-sizing: border-box
	-webkit-font-smoothing: antialiased;
body
	padding: 10px 10px
.container
	width: 100%;
img
	image-rendering: auto;

path.link
	fill: none
	stroke: #666;
marker#licensing
	fill: green;
path.link.licensing
	stroke: green;
path.link.resolved
	stroke-dasharray: 0,2 1;
circle
	stroke: #333;
text
	font: 10px sans-serif
	pointer-events: none;
text.shadow
	stroke: #fff
	stroke-width: 3px
	stroke-opacity: .8;
#map
	width:100%
	height: 700px;



