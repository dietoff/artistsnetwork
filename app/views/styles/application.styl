// nib is a powerful lib for stylus that provides cross-browser css3 mixins.
// No more vendor prefixes. http://visionmedia.github.com/nib/
@import 'nib'

*
	box-sizing: border-box
	-webkit-font-smoothing: antialiased;
html
	overflow-x: hidden !important;

body
	padding: 10px 10px
	padding-right: 0px
	overflow-x: hidden !important
	padding-left: 0px;
.container
	width: 100%;

#main-content
	margin-right: 0px
	margin-left: 0px;
#content
	padding-right: 0px
	padding-left: 0px;
img
	image-rendering: auto;

path.link
	fill: none
	stroke: #666;
marker#licensing
	fill: green;
path.link.licensing
	stroke: green;
path.link.resolved
	stroke-dasharray: 0,2 1;
circle
	stroke: #333;
text
	font: 10px sans-serif
	pointer-events: none;
text.shadow
	stroke: #fff
	stroke-width: 3px
	stroke-opacity: .8;
#map
	width:100%
	height: 700px;
row
	margin-right: -15px
	margin-left: -15px;

